<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.grasp.customview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK" >
    </uses-permission>

    <android:uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <android:uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.LableViewActivity" />
        <activity android:name=".activitys.RoundProgressBarActivity" />
        <activity
            android:name=".activitys.TimerButtonActivity"
            android:label="@string/title_activity_timer_button" >
        </activity>
        <activity
            android:name=".activitys.MultiTouchActivity"
            android:label="@string/title_activity_multi_touch"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".activitys.touch.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.touch.TwoDimensionScrollActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.touch.TwoDimensionGestureScrollActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.touch.MultitouchActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activitys.touch.MoveLoggerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.touch.TouchForwardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.touch.TouchDelegateActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.touch.TouchListenerActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".activitys.touch.TouchInterceptActivity" />
        <activity
            android:name=".activitys.DispalyActivity"
            android:label="@string/title_activity_dispaly" >
        </activity>
        <activity
            android:name=".activitys.CMImageViewActivity"
            android:label="@string/title_activity_cmimage_view" >
        </activity>
        <activity
            android:name=".activitys.device.UniqueIdActivity"
            android:label="@string/title_activity_unique_id" >
        </activity>
        <activity android:name=".activitys.device.MainActivity" >
        </activity>
        <activity
            android:name=".activitys.FlakeActivity"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name=".activitys.ProgressbarActivity"
            android:label="@string/title_activity_progressbar" >
        </activity>
    </application>

</manifest>
